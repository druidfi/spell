{
  "name": "druidfi/spell",
  "description": "Spell to create a new Drupal 8 project",
  "type": "project",
  "homepage": "https://github.com/druidfi/spell",
  "authors": [
    {
      "name": "Marko Korhonen",
      "email": "marko.korhonen@druid.fi"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0",
    "druidfi/d8-template": "dev-composer-project"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "git@github.com:druidfi/d8-template.git"
    }
  ],
  "scripts": {
    "post-create-project-cmd": [
      "rm README.md composer.* .gitignore",
      "mv vendor/druidfi/d8-template/* .",
      "tools/init"
    ]
  }
}